{
  "_version": "1.76.0",
  "sap.app": {
    "id": "com.clientname.zsapui5",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}"
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "dependencies": {
      "minUI5Version": "1.143.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.clientname.zsapui5.i18n.i18n"
        }
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlId": "app",
        "controlAggregation": "pages",
        "viewType": "XML",
        "viewPath": "com.clientname.zsapui5.view",
        "transition": "slide",
        "async": true
      },
      "routes": [
        {
          "name": "RouteView1",
          "pattern": "",
          "target": "TargetView1"
        },
        {
          "name": "RouteView2",
          "pattern": "view2",
          "target": "TargetView2"
        }
      ],
      "targets": {
        "TargetView1": {
          "viewName": "View1",
          "viewId": "View1"
        },
        "TargetView2": {
          "viewName": "View2",
          "viewId": "View2"
        }
      }
    },
    "rootView": {
      "viewName": "com.clientname.zsapui5.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
